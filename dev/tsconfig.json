{
  "compilerOptions": {
    "target": "ESNext",
    "jsx": "react",
    "module": "ESNext",
    "moduleResolution": "node",  // 添加这行
    "esModuleInterop": true,    // 确保这行存在
    "allowSyntheticDefaultImports": true,  // 添加这行
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "typeRoots": [
      "node_modules/@types",
    ],
    "baseUrl": "./",
    "paths": {
      "+/*": [
        "src/component/icons/*"
      ],
      "#/*": [
        "src/component/*"
      ],
      "@/*":[
        "src/*"
      ]
    },
  }
}